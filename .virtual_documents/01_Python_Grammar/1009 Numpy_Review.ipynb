# append()
# axis=1 y축을 기준으로, axis=0 x축을 기준으로
arr1 = np.arange(1,13).reshape(3,4)
arr2 = np.arange(13,25).reshape(3,4)

print(arr1)
print(arr2, end="\n\n")





arr1 = np.arange(1,7).reshape(2,3)
arr2 = np.arange(7,13).reshape(2,3)

print(arr1)
print(arr2)

arr3 = np.vstack((arr1, arr2))
